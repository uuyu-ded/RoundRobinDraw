{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "prefer-spread": "off"
  }
}
